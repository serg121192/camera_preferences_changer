<?xml version="1.0" encoding="UTF-8"?>
<VideoOverlay version="2.0" xmlns="http://www.std-cgi.com/ver20/XMLSchema">
<normalizedScreenSize>
<normalizedScreenWidth>704</normalizedScreenWidth>
<normalizedScreenHeight>576</normalizedScreenHeight>
</normalizedScreenSize>
<attribute>
<transparent>false</transparent>
<flashing>false</flashing>
</attribute>
<TextOverlayList size="8">
</TextOverlayList>
<DateTimeOverlay>
<enabled>true</enabled>
<positionX>{{ DATE_X }}</positionX>
<positionY>{{ DATE_Y }}</positionY>
<dateStyle>{{ DATE_FORMAT }}</dateStyle>
<timeStyle>24hour</timeStyle>
<displayWeek>false</displayWeek>
</DateTimeOverlay>
<channelNameOverlay>
<enabled>true</enabled>
<positionX>{{ CHANNEL_X }}</positionX>
<positionY>{{ CHANNEL_Y }}</positionY>
<name>{{ CHANNEL_NAME }}</name>
</channelNameOverlay>
<fontSize>adaptive</fontSize>
<frontColorMode>auto</frontColorMode>
<frontColor>000000</frontColor>
<alignment>customize</alignment>
<boundary>1</boundary>
<Angle>
<TiltAngle>
<enabled>false</enabled>
<positionX>512</positionX>
<positionY>576</positionY>
</TiltAngle>
</Angle>
<upDownboundary>0</upDownboundary>
<leftRightboundary>0</leftRightboundary>
</VideoOverlay>